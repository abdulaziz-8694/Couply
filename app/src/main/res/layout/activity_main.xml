<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a8aa8a.couply.Activities.MainActivity">

    <Button
        android:id="@+id/jabong_button"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:text="@string/jabong_button"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/myntra_button"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/myntra_button"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:text="@string/myntra_button"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
